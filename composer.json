{
  "name": "silinternational/apiaxle-sdk-php",
  "description": "PHP client for ApiAxle APIs",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "SIL GTIS",
      "email": "gtis_itse_support@sil.org"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4",
    "guzzlehttp/guzzle": "^7.4.5",
    "guzzlehttp/guzzle-services": "^0.6 || ^1.0",
    "forevermatt/calc-api-sig": "^0.1.1",
    "roave/security-advisories": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "satooshi/php-coveralls": "^1.0.1",
    "silinternational/php-env": "^0.2.0"
  },
  "suggest": {
    "roave/security-advisories": "Make sure you don't install dependencies with known issues. Install dev-master version."
  },
  "autoload": {
    "psr-4": {
      "Apiaxle\\": "src/",
      "ApiaxleTests\\": "tests/"
    }
  }
}
